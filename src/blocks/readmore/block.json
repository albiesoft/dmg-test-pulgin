{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dmg/readmore",
	"version": "0.1.0",
	"title": "DMG Read More",
	"category": "text",
	"icon": "smiley",
	"description": "A block to link to specific post",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"link": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "dmg-readmore",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}